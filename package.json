  "scripts": {
    "start": "node backend/src/app.js",
    "dev": "nodemon backend/src/app.js",
    "build": "npm run install:claude-code",
    "install:claude-code": "npm install -g @anthropic-ai/claude-code || npm link @anthropic-ai/claude-code",
    "postinstall": "npm run install:claude-code",
    "lint": "echo 'Linting completed'",
    "test": "npm run test:token-tracking && npm run test:token-api",
    "test:token-tracking": "node backend/test/token-tracking.test.js",
    "test:token-api": "node backend/test/token-usage-api.test.js",
    "test:all": "npm run test:token-tracking && npm run test:token-api"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.0.5",
    "@octokit/app": "^14.0.0",
